<Window x:Class="TimetablingWPF.InformationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimetablingWPF"
        mc:Ignorable="d" Height="400" Width="400"
        Title="Window1" WindowStartupLocation="CenterScreen">
    <StackPanel Orientation="Vertical" Margin="10">
        <WrapPanel Name="wpTop" HorizontalAlignment="Center" Margin="0,0,0,30"/>
        <Grid Name="gdBottom" HorizontalAlignment="Stretch"/>
    </StackPanel>
</Window>
